{
  "manifest_version": 2,
	"name": "Copy Paste",
	"description": "Record your commonly used text.",
	"version": "1.3",
	"permissions": ["storage"],
	"icons": {
		"16": "images/icon16.png",
		"128": "images/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "images/icon16.png",
			"128": "images/icon128.png"
		},
		"default_popup": "popup.html"
	}
}